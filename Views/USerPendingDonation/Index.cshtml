@model IEnumerable<PathToJannah.Models.Donation>


<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.T_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.t_time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Amount)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.T_ID
        </td>
        <td>
            @item.t_time
        </td>
        <td>
            @item.Amount
        </td>
        
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.D_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.D_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.D_ID })
        </td>
    </tr>
}

</table>
